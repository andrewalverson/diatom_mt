mt<-read.csv("intron_exon_similarity.csv",header=TRUE, sep=",")
p <- ggplot(mt,aes(fct_rev(locus),conservation, colour=intron_exon, size=10)) +
guides(size=FALSE) +
geom_jitter(aes(shape=intron_exon), size=12, width=0.2, height=0.2) +
